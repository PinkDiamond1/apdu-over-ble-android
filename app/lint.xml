<?xml version="1.0" encoding="UTF-8"?>
<lint>
  <!-- https://github.com/square/okio/issues/58 -->
  <issue id="InvalidPackage">
    <ignore regexp="okio-1.3.0.jar" />
  </issue>
  <!-- rules below ignore issue in resources generated by google-service plugin -->
  <issue id="MissingTranslation">
    <ignore path="**/google-services/**/values/values.xml" />
  </issue>
  <issue id="UnusedResources">
    <ignore path="**/google-services/**/values/values.xml" />
  </issue>
  <!-- ignore app indexing for now -->
  <issue id="GoogleAppIndexingWarning" severity="ignore" />
</lint>
